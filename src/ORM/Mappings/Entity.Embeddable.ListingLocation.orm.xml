<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance">

    <embeddable name="App\Core\Listing\Entity\Embeddable\ListingLocation">
        <field name="country" nullable="true" type="string" length="50" />
        <field name="city" nullable="true" type="string" length="50" />
        <field name="zipCode" nullable="true" />
        <field name="street" nullable="true" />
        <field name="streetNumber" nullable="true" />
    </embeddable>

</doctrine-mapping>